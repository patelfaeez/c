#include<stdio.h>
void main(){
    float l;
    float b;
    float area;
    float perimeter;
    
    printf("enter l:");
    scanf("%f",&l);
      printf("enter b:");
    scanf("%f",&b);
    
     area=l*b;
     perimeter=2*(l*b);
     
     printf("\narea=%f",area);
     printf("\nperimeter=%f",perimeter);
    
}
