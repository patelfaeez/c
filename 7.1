//printing week by using number//
#include<stdio.h>
void main()
{
    int week;
    printf("enter number of week between (1to7):");
    scanf("%d",&week);
    switch(week)
    {
    case 1:
    printf("monday");
    break;
    case 2:
    printf("tuesday");
    break;
    case 3:
    printf("wednesday");
    break;
      case 4:
    printf("thursday");
    break;
      case 5:
    printf("friday");
    break;
      case 6:
    printf("saturday");
    break;
      case 7:
    printf("sunday");
    break;
    
    default:
    printf("error\n");
    printf("enter numbe between (1 to 7)");
    break;
    }
}
