/*
write a program to accept the value of year asv input from keybord and print whether it is leap or not.
*/
#include<stdio.h>
void main(){
    int year;
    printf("enter year:");
    scanf("%d",&year);
    if(year%4==0)
    {
        printf("year is leap");
    }
    else
    {
        printf("year is not leap");
    }
}

